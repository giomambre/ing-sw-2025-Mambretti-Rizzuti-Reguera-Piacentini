      alignment="CENTER"
      spacing="20.0"
      prefWidth="600.0"
      prefHeight="400.0">

    <Label text="Seleziona una lobby disponibile:"/>

    <ListView fx:id="lobbyListView" prefWidth="300.0" prefHeight="200.0"/>

    <Button text="Entra" onAction="#handleSelect"/>

</VBox>
